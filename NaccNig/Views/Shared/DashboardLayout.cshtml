@using Microsoft.AspNet.Identity
@using NaccNig.Models
@using NaccNigModels.Members
@using NaccNigModels.Structures

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <link href="~/Content/Dashboard/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/Dashboard/css/datepicker3.css" rel="stylesheet">
    <link href="~/Content/Dashboard/css/styles.css" rel="stylesheet">
    <link href="~/Content/Default/pace.css" rel="stylesheet" />
    <!--Icons-->
    <script src="~/Scripts/Dashboard/lumino.glyphs.js"></script>

    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    @RenderBody()
    @Html.Partial("_NavBar")
    @Html.Partial("_SideBar")


    <script src="~/Scripts/Dashboard/jquery-1.11.1.min.js"></script>
    <script src="~/Scripts/Dashboard/bootstrap.min.js"></script>
   
    <script src="~/Scripts/Dashboard/bootstrap-datepicker.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>



