@model IEnumerable<NaccNigModels.Executives.StateExco>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Default.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ActiveMember.Fullname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Portfolio.PortfolioName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StateExcoId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ActiveMember.Fullname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Portfolio.PortfolioName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StateExcoId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
